{
  "name": "gesso-wp",
  "version": "5.2.2",
  "description": "Gesso starter theme",
  "main": "style.css",
  "scripts": {
    "start": "webpack --config ./webpack.theme-config.js && webpack --config ./webpack.dev.js",
    "build": "webpack --config ./webpack.theme-config.js && webpack --config ./webpack.production.js",
    "dev": "npm run start && concurrently --raw \"webpack watch --config ./webpack.dev.js\" \"webpack watch --config ./webpack.theme-config.js\"",
    "rename": "node lib/rename.js",
    "eslint": "eslint -c .eslintrc.js \"**/*.es6.js\"",
    "stylelint": "stylelint \"source/**/*.scss\"",
    "watch": "npm run dev"
  },
  "bugs": "https://github.com/forumone/gesso-wp/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/forumone/gesso-wp.git"
  },
  "author": "Forum One",
  "license": "ISC",
  "homepage": "https://github.com/forumone/gesso-wp#readme",
  "browserslist": [
    "last 2 versions and not dead",
    ">= 1%",
    ">= 1% in US"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@wordpress/babel-preset-default": "^7.9.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^4.8.0",
    "@wordpress/eslint-plugin": "^13.9.0",
    "@wordpress/prettier-config": "^2.8.0",
    "@wordpress/stylelint-config": "^21.8.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "classnames": "^2.3.2",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nani": "^3.2.3",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.3",
    "sass-loader": "^13.2.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@wordpress/base-styles": "^4.16.0",
    "@wordpress/blocks": "^11.21.0",
    "@wordpress/compose": "^5.20.0",
    "@wordpress/dom-ready": "^3.25.0",
    "@wordpress/hooks": "^3.25.0",
    "@wordpress/i18n": "^4.25.0",
    "eslint-webpack-plugin": "^3.2.0",
    "glob": "^8.1.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.2",
    "remove-files-webpack-plugin": "^1.5.0",
    "sass-embedded": "^1.57.1",
    "stylelint-webpack-plugin": "^3.3.0",
    "terser-webpack-plugin": "^5.3.6",
    "webpack-merge": "^5.8.0",
    "yaml": "^1.10.2"
  }
}
